@model IEnumerable<F2022A3THH.Models.MediaTypeBaseViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>List of Media Types</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
    </tr>
}

</table>
